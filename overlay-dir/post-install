#/bin/bash
echo "post-install started"

if [ ! -d ruleset ] ; then
	echo "ignoring this execution, wrong dir"
exit
fi

# I am in debian/

if [ -d script_overlay_was_run  ] ; then
	echo "ignoring this execution, was done already"
exit
fi

mkdir script_overlay_was_run

patch -p 0 < ../../../../patch-gz
